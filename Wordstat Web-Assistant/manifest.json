{
  "manifest_version": 2,
  "browser_action": {
  },
  "background": {
    "page": "background.html"
  },

  "content_scripts": [
    {
      "css": [
        "/css/bootstrap.min.css",
        "/css/style.css"
      ],
      "js": [
        "/js/jquery-3.3.1.min.js",
        "/js/popper.js",
        "/js/bootstrap.min.js",
        "/js/xlsx.full.min.js",
        "/js/content.js"

      ],
      
      "matches": [
        "*://wordstat.yandex.ru/*",
        "*://wordstat.yandex.by/*",
        "*://wordstat.yandex.kz/*",
        "*://wordstat.yandex.ua/*",
        "*://wordstat.yandex.com/*",
        "*://www.wordstat.yandex.ru/*",
        "*://www.wordstat.yandex.by/*",
        "*://www.wordstat.yandex.kz/*",
        "*://www.wordstat.yandex.ua/*",
        "*://www.wordstat.yandex.com/*"
      ]
    }
  ],

  
  "web_accessible_resources": [
    "css/style.css",
    "fonts/Helvetica-Bold.ttf",
    "fonts/dintextpro.ttf",
    "images/logo.png",
    "images/icons/add.png",
    "images/icons/add2.png",
    "images/icons/copy.png",
    "images/icons/copy2.png",
    "images/icons/copy_range.png",
    "images/icons/copy_range2.png",
    "images/icons/delete.png",
    "images/icons/delete2.png",
    "images/icons/download.png",
    "images/icons/download2.png",
    "images/icons/export.png",
    "images/icons/export2.png",
    "images/icons/range.png",
    "images/icons/range2.png",
    "images/icons/minus.png"
  ],
  "version": "1.0.0",
  "name": "Wordstat Web-Assistant",
  "description": "Уникальный плагин, значительно ускоряющий процесс сбора ключевых слов и минус слов в сервисе wordstat.yandex.ru",
  "icons": {
    "128": "images/logo.png",
    "16": "images/logo.png",
    "48": "images/logo.png"
  }
  
}